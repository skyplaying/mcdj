/* 内容模块3 */
.container3{
    padding: 90rpx 20rpx 140rpx;
    background: url(http://image.qipinke.com/icon/family_flow_bg.png) #FCEFCF no-repeat top center;
    background-size: 750rpx 1260rpx;
}
.welcome-wrap{
    width: 416rpx;
    margin-left: 10rpx;
}
.wrap_hd{
    position: relative;
    line-height: 100rpx;
    font-size: 32rpx;
    font-weight: bold;
    width: 150rpx;
    text-align: center;
    margin: 0 auto;
}
.wrap_hd::after,.wrap_hd::before{
    content: ' ';
    position: absolute;
    top: 50%;
    width: 20rpx;
    height: 4rpx;
    margin-top: -2rpx;
    background-color: #FF9800;
}
.wrap_hd::after{
    right: 100%;
}
.wrap_hd::before{
    left: 100%;
}
.require-wrap{
    background-color: white;
    padding: 0 40rpx 46rpx;
    margin-bottom: 14rpx;
    line-height: 46rpx;
    margin-top: 626rpx;
}
.process-wrap{
    background-color: white;
    padding-bottom: 46rpx;
    margin-bottom: 14rpx;
    text-align: center;
}
.process-img{
    width: 210rpx;
    height: 392rpx;
    margin-top: 10rpx;
}
